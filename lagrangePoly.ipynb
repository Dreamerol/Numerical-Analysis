   "source": [
    "def basisPoly(idx, nodes, x):\n",
    "    y = 1\n",
    "    for i in range(0, len(nodes)):\n",
    "        if i == idx:\n",
    "            continue\n",
    "        y *=((x - nodes[i])/(nodes[idx] - nodes[i]))\n",
    "    return y\n",
    "\n"
   ]
  }
